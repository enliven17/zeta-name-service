{
  "name": "credit-name-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "hardhat compile",
    "deploy:arbitrum": "hardhat run scripts/deploy-omnichain.js --network arbitrumSepolia",
    "deploy:zeta": "hardhat run scripts/deploy-omnichain.js --network zetachainTestnet",
    "deploy:ethereum": "hardhat run scripts/deploy-omnichain.js --network ethereumSepolia",
    "deploy:bsc": "hardhat run scripts/deploy-omnichain.js --network bscTestnet",
    "deploy:polygon": "hardhat run scripts/deploy-omnichain.js --network polygonMumbai",
    "deploy:local": "hardhat run scripts/deploy-omnichain.js --network localhost",
    "deploy:all": "npm run deploy:arbitrum && npm run deploy:zeta && npm run deploy:ethereum",
    "deploy:all-testnets": "npm run deploy:arbitrum && npm run deploy:zeta && npm run deploy:ethereum && npm run deploy:bsc && npm run deploy:polygon",
    "test": "hardhat test",
    "test:crosschain:arbitrum": "hardhat run scripts/test-crosschain.js --network arbitrumSepolia",
    "test:crosschain:zeta": "hardhat run scripts/test-crosschain.js --network zetachainTestnet",
    "debug:crosschain:arbitrum": "hardhat run scripts/debug-crosschain.js --network arbitrumSepolia",
    "debug:crosschain:zeta": "hardhat run scripts/debug-crosschain.js --network zetachainTestnet",
    "configure:zeta:arbitrum": "hardhat run scripts/configure-zeta.js --network arbitrumSepolia",
    "configure:zeta:zeta": "hardhat run scripts/configure-zeta.js --network zetachainTestnet"
  },
  "dependencies": {
